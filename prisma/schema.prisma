generator client {
  provider = "prisma-client"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "postgresql"
}

model Job {
  id String @id @default(uuid())
  clerkId String
  position String
  company String
  location String
  status String
  type String
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}